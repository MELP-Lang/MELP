{
  "name": "mlp-konva-editor",
  "version": "1.0.0",
  "description": "High-Performance MLP GUI Editor with Konva.js",
  "main": "src/main/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "npm run build && electron-builder",
    "start": "electron ."
  },
  "keywords": ["mlp", "gui", "editor", "konva", "canvas", "electron"],
  "author": "MLP Team",
  "license": "MIT",
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "konva": "^9.3.6",
    "react-konva": "^18.2.10"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "vite": "^5.4.2",
    "electron": "^27.1.3",
    "electron-builder": "^24.13.3",
    "concurrently": "^8.2.2",
    "wait-on": "^7.2.0"
  }
}
