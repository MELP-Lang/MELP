-- YZ_29 Test: Mixed qualified and unqualified enum access
enum TokenType
    T_FUNCTION = 1
    T_IF = 3
end_enum

enum Status
    ACTIVE = 10
    PENDING = 20
end_enum

function main() returns numeric
    -- Unqualified access (YZ_29)
    numeric a = T_FUNCTION
    numeric b = T_IF
    
    -- Qualified access (YZ_96)
    numeric c = Status.ACTIVE
    numeric d = Status.PENDING
    
    -- Mixed calculation
    numeric result = a + b + c + d
    -- Expected: 1 + 3 + 10 + 20 = 34
    
    println("Mixed enum test:")
    print(result)
    
    return result
end_function
