; Function: skip_whitespace
define i8* @skip_whitespace(i8* %source, i64 %pos, i64 %line, i64 %col) {
entry:
    %source_len = alloca i64, align 8
    %tmp67 = call i64 @melp_list_length(i8* %source)
    store i64 %tmp67, i64* %source_len, align 8
  %ch_ptr = alloca i8*, align 8
  %tmp68 = getelementptr inbounds [1 x i8], [1 x i8]* @.str.1, i64 0, i64 0
  store i8* %tmp68, i8** %ch_ptr, align 8
    %ch_is_whitespace = alloca i64, align 8
    store i64 0, i64* %ch_is_whitespace, align 8
    br label %label133
label133:
    %tmp69 = load i64, i64* %source_len, align 8
    %tmp70 = icmp slt i64 %pos, %tmp69
    br i1 %tmp70, label %label134, label %label135
label134:
    br label %label133
label135:
    %tmp71 = call i64 @substring(i8* %source, i64 %pos, i64 1)
    store i64 %tmp71, i64* %ch, align 8
    %tmp72 = load i8*, i8** %ch, align 8
    %tmp73 = call i64 @is_whitespace(i8* %tmp72)
    store i64 %tmp73, i64* %ch_is_whitespace, align 8
    %tmp74 = load i64, i64* %ch_is_whitespace, align 8
    %tmp75 = icmp eq i64 %tmp74, 1
    br i1 %tmp75, label %label136, label %label137
label136:
    %tmp76 = load i64, i64* %ch, align 8
    %tmp77 = icmp eq i64 %tmp76, 0
    br i1 %tmp77, label %label139, label %label140
label139:
    %tmp78 = add nsw i64 %line, 1
    store i64 %tmp78, i64* %line, align 8
    store i64 1, i64* %col, align 8
    %tmp79 = add nsw i64 %pos, 1
    store i64 %tmp79, i64* %pos, align 8
    br label %label141
label140:
    %tmp80 = add nsw i64 %col, 1
    store i64 %tmp80, i64* %col, align 8
    %tmp81 = add nsw i64 %pos, 1
    store i64 %tmp81, i64* %pos, align 8
    br label %label141
label141:
    br label %label138
label137:
    br label %label138
label138:
}

