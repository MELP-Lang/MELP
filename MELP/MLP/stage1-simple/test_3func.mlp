-- Stage1-Simple: 3-Function Test

function tokenize(size)
    Numeric count
    count = size + 5
    return count
end_function

function parse(tokens)
    Numeric nodes
    nodes = tokens + 3
    return nodes
end_function

function main()
    Numeric t
    Numeric n
    t = tokenize(10)
    n = parse(t)
    return n
end_function
