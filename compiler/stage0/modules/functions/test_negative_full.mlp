function main() returns numeric
    numeric x = -15
    numeric y = -20
    numeric z = x + y
    numeric a = -5
    numeric b = 10
    numeric c = a + b
    
    if z == -35 then
        println("Test 1 PASSED: -15 + -20 = -35")
    else
        println("Test 1 FAILED")
    end if
    
    if c == 5 then
        println("Test 2 PASSED: -5 + 10 = 5")
    else
        println("Test 2 FAILED")
    end if
    
    numeric neg_zero = -0
    if neg_zero == 0 then
        println("Test 3 PASSED: -0 = 0")
    else
        println("Test 3 FAILED")
    end if
    
    return 0
end function
