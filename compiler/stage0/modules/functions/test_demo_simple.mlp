function calculate(numeric a, numeric b) returns numeric
    numeric sum = a + b
    numeric product = a * b
    numeric out1 = println(sum)
    numeric out2 = println(product)
    return sum
end function

function count_up(numeric limit) returns numeric
    numeric i = 0
    while i < limit
        i = i + 1
    end while
    return i
end function

