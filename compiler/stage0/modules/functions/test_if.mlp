-- MLP Comprehensive If/Else Test
-- Find max of two numbers

function max(numeric a, numeric b) returns numeric
    if a > b then
        return a
    else
        return b
    end if
end function

function main()
    numeric x = 15
    numeric y = 23
    
    numeric result = max(x, y)
    print result
    
    return 0
end function
