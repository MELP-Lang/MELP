-- Test if/else with different scenarios

function get_abs(numeric x) returns numeric
    if x < 0
        return 0 - x
    else
        return x
    end if
end function

function main()
    numeric a = -5
    numeric b = 10
    
    numeric r1 = get_abs(a)
    print r1
    
    numeric r2 = get_abs(b)
    print r2
    
    return 0
end function
