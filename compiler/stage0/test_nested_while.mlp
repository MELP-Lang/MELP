-- Test nested while loops
-- Outer while contains inner while - tests label management

function test_nested_loops() returns numeric
    numeric i = 0
    numeric j = 0
    numeric count = 0
    
    while i < 3 do
        j = 0
        -- Inner while loop
        while j < 2 do
            count = count + 1
            j = j + 1
        end while
        i = i + 1
    end while
    
    return count
end function
