function scan_number() returns string
    numeric pos = 0
    string num_value = ""
    string source = "123"
    numeric source_len = 3
    string ch = ""
    
    while pos < source_len
        ch = substring(source, pos, 1)
        
        if ch == "1" then
            num_value = num_value + ch
            pos = pos + 1
        else
            if ch == "." then
                if pos < 2 then
                    num_value = num_value + ch
                    pos = pos + 1
                else
                    exit while
                end if
            else
                exit while
            end if
        end if
    end while
    
    return num_value
end_function

function main() returns numeric
    string result = ""
    result = scan_number()
    return 0
end_function
